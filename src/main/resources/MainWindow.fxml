<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox fx:controller="MainWindow" xmlns:fx="http://javafx.com/fxml" stylesheets="MainWindow.css" spacing="30" prefHeight="400" prefWidth="500" alignment="CENTER">
    
    <Label fx:id="title" />
    <Button onAction="#onPlayClicked" text="Play" alignment="CENTER" />
    <Button onAction="#onLoadClicked" text="Load" alignment="CENTER" />
    <Button onAction="#onHighScoreClicked" text="High Score" alignment="CENTER" />

</VBox>